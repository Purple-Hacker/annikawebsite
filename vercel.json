{
    "cleanUrls": true,
    "trailingSlash": true,
    "headers": [
        {
            "source": "/assets/(.*).pdf",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/pdf"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000"
                }
            ]
        }
    ]
}